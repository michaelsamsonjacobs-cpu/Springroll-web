{
    "productName": "Springroll Team",
    "version": "0.5.2",
    "identifier": "ai.springroll.team",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "node scripts/build-tauri.js",
        "devUrl": "http://localhost:5173/app/",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "Springroll Team",
                "width": 1080,
                "height": 720,
                "decorations": true,
                "transparent": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/icon.ico"
        ],
        "externalBin": [
            "binaries/ollama"
        ],
        "shortDescription": "Sovereign AI Agentic Workstation",
        "longDescription": "",
        "copyright": "",
        "category": "Productivity"
    }
}